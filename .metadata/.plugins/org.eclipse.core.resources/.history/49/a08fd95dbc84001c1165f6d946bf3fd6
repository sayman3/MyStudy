package Array;

public class Practice {

	public static void main(String[] args) {

		int[] names; //배열을 선언 -> 배열의 주소만 배정된다.
		names = new int[5]; //배열을 생성 -> 저장할 공간을 생성한다.
		
		int[] once = new int[3]; //배열의 선언과 생성을 한번에 한다.
		int[] birthDay = new int[] {9,12,12};//배열의 생성과 동시에 초기화.
		System.out.println(birthDay[1]);
	}

}
